{
    "title": "FrontmostIfDiscord_summary",
    "rules": [
      {
        "description": "FrontmostIfDiscord_summary",
        "manipulators": [
          {
            "description": "J_DownArrow",
            "type": "basic",
            "from": <%= from("j",["caps_lock"]) %>,
            "to": <%= to([["down_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "K_UpArrow",
            "type": "basic",
            "from": <%= from("k",["caps_lock"]) %>,
            "to": <%= to([["up_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "M_最終行へ",
            "type": "basic",
            "from": <%= from("m",["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "N_最初の行へ",
            "type": "basic",
            "from": <%= from("n",["caps_lock"]) %>,
            "to": <%= to([["left_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "L_RightArrow",
            "type": "basic",
            "from": <%= from("l", ["caps_lock"]) %>,
            "to": <%= to([["right_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "O(オー)_PageUp",
            "type": "basic",
            "from": <%= from("o", ["caps_lock"]) %>,
            "to": <%= to([["up_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "i_PageDown",
            "type": "basic",
            "from": <%= from("i", ["caps_lock"]) %>,
            "to": <%= to([["down_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "Slash_拡大",
            "type": "basic",
            "from": <%= from("slash", ["caps_lock"]) %>,
            "to": <%= to([["equal_sign", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "B_縮小",
            "type": "basic",
            "from": <%= from("b", ["caps_lock"]) %>,
            "to": <%= to([["hyphen", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "T_単語検索",
            "type": "basic",
            "from": <%= from("t", ["caps_lock"]) %>,
            "to": <%= to([["f", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "H_LeftArrow",
            "type": "basic",
            "from": <%= from("h", ["caps_lock"]) %>,
            "to": <%= to([["left_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "Period_MaxWindow",
            "type": "basic",
            "from": <%= from("period", ["caps_lock"]) %>,
            "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "Comma_AppsSelect-Next",
            "type": "basic",
            "from": <%= from("comma", ["caps_lock"]) %>,
            "to": <%= to([["tab", "right_option"],["caps_lock"]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "Quote_PreviewActiveWindow",
            "type": "basic",
            "from": <%= from("quote", ["caps_lock"]) %>,
            "to": <%= set_shell_command(["open -a 'Preview.app'"]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "U_Undo",
            "type": "basic",
            "from": <%= from("u", ["caps_lock"]) %>,
            "to": <%= to([["z", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
              "description": "R_miアクテイブ",
            "type": "basic",
            "from": <%= from("r", ["caps_lock"]) %>,
            "to": <%= set_shell_command(["open -a 'mi'"]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "P_RightHalfWindowMove",
            "type": "basic",
            "from": <%= from("p", ["caps_lock"]) %>,
            "to": <%= to([["3", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "Q_LeftHalfWindowMove",
            "type": "basic",
            "from": <%= from("q", ["caps_lock"]) %>,
            "to": <%= to([["2", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          },
          {
            "description": "D_WindowTopHalf",
            "type": "basic",
            "from": <%= from("d", ["caps_lock"]) %>,
            "to": <%= to([["4", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("discord") %> ]
          }
        ]
      }
    ]
}