{
    "title": "FrontmostIfPathFinder_summary",
    "rules": [
      {
        "description": "FrontmostIfPathFinder_summary",
        "manipulators": [
          {
            "description": "LeftCommand + Quote_右の枠へ移動-左の枠へ移動",
            "type": "basic",
            "from": <%= from("quote", ["left_control"]) %>,
            "to": <%= to([["f6"]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + U_PathSelection",
            "type": "basic",
            "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["l", ["left_alt", "left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + O(オー)_一括名称変更",
            "type": "basic",
            "from": <%= from("o", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["r", ["left_gui", "left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "RightCommand + Space_FileDelete",
            "type": "basic",
            "from": <%= from("spacebar", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["delete_or_backspace", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "RightOption + E_エイリアスを作る",
            "type": "basic",
            "from": <%= from("e", ["right_alt"], ["caps_lock"]) %>,
            "to": <%= to([["l", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftCommand + Z_オリジナルを表示",
            "type": "basic",
            "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftCommand + T_左シェルフ",
            "type": "basic",
            "from": <%= from("t", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["u", ["left_control", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "RightCommand + H_フォルダーへ移動",
            "type": "basic",
            "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["g", ["left_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + F_NewFileCreate",
            "type": "basic",
            "from": <%= from("f", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_alt", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + A_FolderCreate",
            "type": "basic",
            "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + Period_CurrentFolderTerminalOpen",
            "type": "basic",
            "from": <%= from("period", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["period", ["left_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftAlt + Comma_SelectFilePathCopy",
            "type": "basic",
            "from": <%= from("comma", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["backslash", ["left_control", "left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftOption + SpaceBar_FileOpen",
            "type": "basic",
            "from": <%= from("spacebar", ["left_option"], ["caps_lock"]) %>,
            "to": <%= to([["o", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftOption + I_親ディレクトリへ",
            "type": "basic",
            "from": <%= from("i", ["left_option"], ["caps_lock"]) %>,
            "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "RightOption + R_デュアルウィンドウトグル",
            "type": "basic",
            "from": <%= from("r", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["f1"]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftCommand + O(オー)_垂直レイアウト",
            "type": "basic",
            "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["o", ["left_control", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftCommand + 0ゼロ_水平レイアウト",
            "type": "basic",
            "from": <%= from("0", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["d", ["right_option", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftCommand + N_HeadRowCursol",
            "type": "basic",
            "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["left_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "LeftCommand + M_EndRowCursol",
            "type": "basic",
            "from": <%= from("m", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "RightCommand + A_新規タブ",
            "type": "basic",
            "from": <%= from("a", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["t", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "RightOption + A_新規ブラウザ",
            "type": "basic",
            "from": <%= from("a", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["n", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "RightCommand + L_前のタブを選択",
            "type": "basic",
            "from": <%= from("l", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["open_bracket", ["right_gui","right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "RightCommand + Semicolon_次のタブを選択",
            "type": "basic",
            "from": <%= from("semicolon", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["close_bracket", ["right_gui","right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "RightOption + T_タブセット",
            "type": "basic",
            "from": <%= from("t", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["p", ["right_gui","left_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %>]
          },
          {
            "description": "Fn + U_WindowLeftUp",
            "type": "basic",
            "from": <%= from("u", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["6", "right_alt"]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + J_WindowLeftDown",
              "type": "basic",
              "from": <%= from("j", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["8", "right_alt"]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + I_WindowRightUp",
              "type": "basic",
              "from": <%= from("i", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["7", "right_alt"]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + K_WindowRightDown",
              "type": "basic",
              "from": <%= from("k", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["9", "right_alt"]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + G_WindowLeftOneThird",
              "type": "basic",
              "from": <%= from("g", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["d", ["right_control", "right_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + Quote_WindowRightOneThird",
              "type": "basic",
              "from": <%= from("quote", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["g", ["right_control", "right_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + Y_WindowLeftTwo-thirds",
              "type": "basic",
              "from": <%= from("y", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["e", ["right_control", "right_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + O_WindowRightTwo-thirds",
              "type": "basic",
              "from": <%= from("o", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["t", ["right_control", "right_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + Comma_WindowCenter",
              "type": "basic",
              "from": <%= from("comma", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["c", ["right_control", "right_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "RightCommand + Period_MaxWindow",
            "type": "basic",
            "from": <%= from("period", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + N_WindowUP",
              "type": "basic",
              "from": <%= from("n", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["4", ["left_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
              "description": "Fn + M_WindowDown",
              "type": "basic",
              "from": <%= from("m", ["right_control"], ["caps_lock"]) %>,
              "to": <%= to([["5", ["left_alt"]]]) %>,
              "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "Fn + L_RightHalfWindowMove",
            "type": "basic",
            "from": <%= from("l", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "Fn + H_LeftWindowMove",
            "type": "basic",
            "from": <%= from("h", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["2", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "使用しない_LeftOption + O(オー)_FileOpen",
            "type": "basic",
            "from": <%= from("o", ["left_option"], ["caps_lock"]) %>,
            "to": <%= to([["o", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
          },
          {
            "description": "LeftControl_Rename(pathfinder)",
            "type": "basic",
            "from": {
              "apple_vendor_top_case_key_code":"keyboard_fn",
            "modifiers": {
                "optional": [
                  "caps_lock"
                ]
              }
            },
            "to": [
              {
                "key_code": "return_or_enter"
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^com\\.cocoatech\\.PathFinder$"
                ]
              }
            ]
          }
        ]
      }
    ]
}