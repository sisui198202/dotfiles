{
  "title": "ShotCutKey98",
  "rules": [
    {
      "description": "RightOption + L_ダウンロードディレクトリへ98",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "left_shift",
                "left_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
