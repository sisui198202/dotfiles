{
  "title": "Replacekey",
  "rules": [
        {
      "description": "Leftoption + Space to Enter(!Finder)",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["spacebar"],
                 from_mandatory_modifiers: ["left_alt"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["return_or_enter"],
                 conditions: [frontmost_application_unless("finder", false)],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand + f_Tab",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["f"],
                 from_mandatory_modifiers: ["left_control"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["tab"],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand + space_BackSpace(!Finder)",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["spacebar"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: [["japanese_eisuu","delete_or_backspace"]],
                 conditions: [frontmost_application_unless("finder", false)],
                 as_json: true
               )
       %>        
    },
    {
      "description": "LeftCommand + Rightoption_equal",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_alt"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: [["japanese_eisuu","equal_sign"]],
                 as_json: true
               )
       %>
    },
    <%# deleteを機能させるには、anyが必要%>
    {
      "description": "RightCommand + Space_delete(!Finder)",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["spacebar"],
                 from_mandatory_modifiers: ["right_gui"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["delete_forward"],
                 conditions: [frontmost_application_unless("finder", false)],
                 as_json: true
               )
       %>
    },
    {
      "description": "RightCommand + Leftoption_backslash",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_alt"],
                 from_mandatory_modifiers: ["right_gui"],
                 dest_keys_list: [["japanese_eisuu","backslash"]],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand + g_escape",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["g"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["escape"],
                 as_json: true
               )
       %>
    },
    <%# shift + 矢印キーを機能させるには、anyが必要%>
    {
      "description": "LeftCommand + I/J/K/L _Arrows",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["i","j","k","l"],
                 from_mandatory_modifiers: ["left_control"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["up_arrow","left_arrow","down_arrow","right_arrow"],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand+F_Tab",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["f"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["tab"],
                 as_json: true
               )
       %>
    },
    <%# "lazy": true必要なし、あると機能しなくなる %>
    {
      "description": "LeftAlt_Copy",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_alt"],
                 dest_keys_list: ["left_alt"],
                 to_if_alone: ["key_code": "c","modifiers": ["right_gui"]],
                 as_json: true
               )
       %>
    },
   <%# "lazy": true必要なし、あると機能しなくなる %>
    {
      "description": "RightOption_Paste",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_alt"],
                 dest_keys_list: ["right_alt"],
                 to_if_alone: ["key_code": "v","modifiers": ["left_gui"]],
                 as_json: true
               )
       %>
    },
    {
      "description": "Leftoption + Rightoption_parsent",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_alt"],
                 from_mandatory_modifiers: ["left_alt"],
                 dest_keys_list: ["5"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
    {
      "description": "Rightoption + Leftoption_anbasado",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_alt"],
                 from_mandatory_modifiers: ["right_alt"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["7"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
{
                        "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな）",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control"
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_eisuu"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command"
                                },
                                "to": [
                                    {
                                        "key_code": "right_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_kana"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
    {
      "description": "LeftCommand + RightCommand_plus",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_gui"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["equal_sign"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
   {
      "description": "RightCommand + LeftCommand_underbar",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_control"],
                 from_mandatory_modifiers: ["right_gui"],
                 dest_keys_list: ["hyphen"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
    {
      "description": "Leftoption + Rightcommand_hypen",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_gui"],
                 from_mandatory_modifiers: ["left_alt"],
                 dest_keys_list: [["japanese_eisuu","hyphen"]],
                 as_json: true
               )
       %>
    }
  ]
}